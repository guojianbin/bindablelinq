<Application x:Class="Bindable.Linq.Samples.Browser.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    StartupUri="Window1.xaml">
    <Application.Resources>
         
    	<ResourceDictionary>
    		<ResourceDictionary.MergedDictionaries>
    			<ResourceDictionary Source="Shared\Resources\Borders.xaml"/>
    			<ResourceDictionary Source="Shared\Resources\TabPanels.xaml"/>
    			<ResourceDictionary Source="Shared\Resources\Brushes.xaml"/>
    			<ResourceDictionary Source="Shared\Resources\Colors.xaml"/>
    			<ResourceDictionary Source="Shared\Resources\Layouts.xaml"/>
    		</ResourceDictionary.MergedDictionaries>
    	</ResourceDictionary>
         
    </Application.Resources>
</Application>
