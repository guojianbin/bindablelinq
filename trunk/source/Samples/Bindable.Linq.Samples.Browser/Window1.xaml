<Window 
    x:Class="Bindable.Linq.Samples.Browser.Window1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Window1" Height="480" Width="640"
    WindowState="Maximized"
    >
    <DockPanel>
    	<TabControl Style="{DynamicResource TabPanel_OfficeOptions}" IsSynchronizedWithCurrentItem="True" Margin="7,7,7,7">
    		<TabItem Header="TabItem">
    			<Grid/>
    		</TabItem>
    		<TabItem Header="TabItem">
    			<Grid/>
    		</TabItem>
    	</TabControl>
        
    </DockPanel>
</Window>
